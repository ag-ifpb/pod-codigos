
public class Main3 {
	private static int v = 0;

	public static void main(String[] args) {
		Thread t1 = new Thread(){
			public void run() {
				v = 2;
				v = v + 2;
			};
		};
		
		Thread t2 = new Thread(){
			public void run() {
				v = 3;
				v = v + 3;
			};
		};
		//
		t1.start();
		t2.start();
		//
		System.out.println("Valor de v: " + v);//<-- 0, 4, 6, 7 e 8 
		
		
	}
}
