
public class Main5 {

	public static void main(String[] args) {
		System.out.println(Runtime.getRuntime().availableProcessors());
		//
		final Thread t0 = new Thread(){
			public void run() {
				System.out.println("Fui notificado: " + t0.getName());
			};
		};
		final Thread t1 = new Thread(){
			public void run() {
				System.out.println("Fui notificado: " + t1.getName());
			};
		};
		final Thread t2 = new Thread(){
			public void run() {
				System.out.println("Fui notificado: " + t2.getName());
				
			};
		};
	}
}
