package ifpb;

import javax.xml.bind.JAXB;

public class Main {

	public static void main(String[] args) {
		//
		RemoteRef ref = new RemoteRef();
		ref.setHost("123.092.222.121");
		ref.setPort(10999);
		//
		Message m = new Message();
		m.setId(12231);
		m.setRef(ref);
		//
		JAXB.marshal(m, System.out);
	}
}
