package ag.ifpb.impl;

import ag.ifpb.EncriptionType;
import ag.ifpb.EncrypterFactory;
import ag.ifpb.EncryptionStrategy;

public class EncrypterFactoryImpl implements EncrypterFactory {

	@Override
	public EncryptionStrategy strategy(EncriptionType type) {
		//
		EncryptionStrategy strategy = new EncryptionStrategy() {
			
			@Override
			protected EncriptionType type() {
				return type;
			}
			
			@Override
			protected String encrTwo(int key, String text) {
				type()
				return null;
			}
			
			@Override
			protected String encrThree(int key, String text) {
				// TODO Auto-generated method stub
				return null;
			}
			
			@Override
			protected String encrOne(int key, String text) {
				// TODO Auto-generated method stub
				return null;
			}
		};
		return null;
	}

}
